<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}IT Lighthouse{% endblock %}</title>
    <link rel="icon"
          href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
    <header>
        <nav class="navbar navbar-expand-lg navbar-light bg-light">
            <div class="container">
                <a class="navbar-brand" href="#">
                    <img class="logo " src="{{ asset('images/logopng.png') }}" alt="logo">
                </a>
            </div>

            <ul class="navbar-nav">
                <li class="nav-item"><a class="nav-link" href="{{ path('app_home') }}">Home</a></li>
                <li class="nav-item"><a class="nav-link" {{ path('app_home') }}>Nieuws</a></li>
                <li class="nav-item"><a class="nav-link" href="#apply">Evenementen</a></li>
                <li class="nav-item"><a class="nav-link" href="{{ path('app_sponsors') }}">Sponsoren</a></li>
                <li class="nav-item"><a class="nav-link" href="{{ path('app_initiatives') }}">Initiatieven</a></li>
                <li class="nav-item"><a class="nav-link" {{ path('app_home') }}>Bestuursleden</a></li>
            </ul>
        </nav>
    </header>

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body>
{% block body %}{% endblock %}
</body>
</html>
